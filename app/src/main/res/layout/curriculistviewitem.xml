<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="40px"
    android:paddingTop="40px">


    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/kctime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/curriculistview_item_timeStrar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#005982"
            android:textSize="50px" />


    </com.zhy.autolayout.AutoLinearLayout>

    <com.weiye.myview.MyGridView
        android:id="@+id/curriculistview_item_gridview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/kctime"
        android:layout_marginTop="30px"
        android:horizontalSpacing="40px"
        android:numColumns="3"
        android:verticalSpacing="40px" />
</com.zhy.autolayout.AutoRelativeLayout>
